{
  "title": "100k testnet midrange",
  "timeout": 3600,
  "resources": [
    {
      "type": "esdump",
      "url": "http://storage.telos.net/test-resources/telos-testnet-199m-100k.tar.zst",
      "destinationPath": "telos-testnet-199m-100k.tar.zst",
      "decompressPath": "telos-testnet-199m-100k"
    }
  ],
  "elastic": {
    "host": "http://127.0.0.1:9200",
    "esDumpLimit": 4000
  },
  "translator": {
    "template": "config.testnet.json",
    "partial": {
      "chainName": "telos-testnet-verification",
      "startBlock": 199000000,
      "runtime": {
        "reindex": {
          "into": "telos-testnet-reindex",
          "trimFrom": 199000000
        }
      },
      "perf": {
        "elasticDumpSize": 25000
      },
      "elastic": {
        "scrollSize": 25000,
        "scrollWindow": "15m"
      }
    },
    "totalBlocks": 100000
  }
}
