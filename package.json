{
  "name": "telosevm-translator",
  "version": "1.0.0-rc10",
  "description": "State History EVM Translator/Indexer",
  "author": "guilledk",
  "license": "MIT",
  "repository": "https://github.com/telosnetwork/telosevm-translator",
  "scripts": {
    "bootstrap": "yarn",
    "build": "yarn run bootstrap && npx tsc && node scripts/create-dist-packages.mjs",
    "db-check": "yarn run build && node build/main.js --only-db-check",
    "start": "yarn run build && node build/main.js",
    "start-bg": "sh -c 'nohup node \"$(pwd)/build/main.js\" 2>&1 >> run.log &'",
    "test-mock": "yarn run build && mocha build/tests/leap.mock.spec.js --timeout 120000 --debug-brk",
    "test-unit": "yarn run build && mocha build/tests/unit/**/*.unit.spec.js --debug-brk",
    "coverage": "nyc mocha build/tests/*.unit.spec.js",
    "elastic-start": "node scripts/docker-cli.mjs start elastic",
    "elastic-stop": "node scripts/docker-cli.mjs stop elastic",
    "kibana-start": "node scripts/docker-cli.mjs start kibana",
    "kibana-stop": "node scripts/docker-cli.mjs stop kibana",
    "docker-start": "node scripts/docker-cli.mjs start translator",
    "docker-stop": "node scripts/docker-cli.mjs stop translator",
    "toxi-start": "node scripts/docker-cli.mjs start toxiproxy",
    "toxi-stop": "node scripts/docker-cli.mjs stop toxiproxy",
    "clean-deps": "rm -rdf node_modules && yarn cache clean",
    "clean-build": "rm -rdf build/**/*.js build/*.js build/**/*.map"
  },
  "type": "module",
  "dependencies": {
    "@elastic/elasticsearch": "8.11.0",
    "@ethereumjs/block": "5.0.1",
    "@ethereumjs/common": "4.1.0",
    "@ethereumjs/tx": "5.1.0",
    "@ethereumjs/util": "9.0.1",
    "@ethereumjs/vm": "7.1.0",
    "@greymass/eosio": "0.7.0",
    "@telosnetwork/hyperion-sequential-reader": "1.2.7-rc4",
    "@telosnetwork/telos-evm-custom-ds": "1.0.2",
    "apache-arrow": "^15.0.0",
    "bufferutil": "^4.0.8",
    "commander": "^11.1.0",
    "eosjs": "22.1.0",
    "ethereum-cryptography": "2.1.2",
    "lodash.clonedeep": "^4.5.0",
    "moment": "2.29.4",
    "node-fetch": "2.6.9",
    "nodejs-polars": "^0.8.4",
    "rlp": "3.0.0",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.38.0",
    "uuid": "^9.0.1",
    "winston": "3.8.2",
    "workerpool": "^8.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "chai": "^4.3.10",
    "depcheck": "^1.4.7",
    "elasticdump": "^6.110.0",
    "leap-mock": "0.1.7",
    "lzma-native": "^8.0.6",
    "mocha": "^10.2.0",
    "nodejs-file-downloader": "^4.12.1",
    "nyc": "^15.1.0",
    "pretty-bytes": "^6.1.1",
    "progress-stream": "^2.0.0",
    "simple-zstd": "^1.4.2",
    "tar": "^6.2.0",
    "toxiproxy-node-client": "^3.0.0",
    "typescript": "4.9.5"
  }
}
